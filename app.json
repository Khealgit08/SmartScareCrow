{
  "expo": {
    "name": "SmartCrow",
    "slug": "SmartCrow",
    "scheme": "smartcrow",
    "version": "1.0.0",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5e60bdbf-3938-471a-8737-b5dc4c859960"
    },
    "orientation": "portrait",
    "icon": "./assets/image/Picture1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-secure-store",
      "expo-image-picker"
    ],
    "splash": {
      "image": "./assets/image/Picture1.png",
      "resizeMode": "contain",
      "backgroundColor": "#E53E3E"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone",
        "NSLocationWhenInUseUsageDescription": "Allow $(PRODUCT_NAME) to access your location",
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos to change profile picture.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.smartcrow.app"
    },
    "android": {
      "package": "com.smartcrow.app",
      "versionCode": 1,
      "intentFilters": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/image/Picture1.png",
        "backgroundColor": "#E53E3E"
      },
      "splash": {
        "image": "./assets/image/Picture1.png",
        "resizeMode": "contain",
        "backgroundColor": "#E53E3E"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/image/Picture1.png"
    },
    "extra": {
      "eas": {
        "projectId": "5e60bdbf-3938-471a-8737-b5dc4c859960"
      }
    }
  }
}