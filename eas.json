{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production"
      },
      "node": "18.18.0",
      "npm": "10.2.0",
      "buildCommand": "npm install --legacy-peer-deps && npm run build"
    }
  },
  "submit": {
    "production": {}
  }
}
